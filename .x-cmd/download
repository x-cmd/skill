
fork_from_anthropic(){
    ___x_cmd git clone --depth 1 https://github.com/anthropics/skills
    (
        ___x_cmd cd skills/skills
        ___x_cmd rm -rf README.md
    )
    ___x_cmd rm -rf data/anthropics
    ___x_cmd mv skills/skills data/anthropics

    ___x_cmd rm -rf data/anthropics/docx data/anthropics/pdf data/anthropics/pptx data/anthropics/xlsx
    ___x_cmd rm -rf skills
}

